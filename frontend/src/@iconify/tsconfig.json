{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "declaration": false,
        "declarationMap": false,
        "sourceMap": false,
        "composite": false,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "verbatimModuleSyntax": true,
        "allowJs": true,
        "noEmit": true
    },
    "include": [
        "./icons-bundle.js"
    ]
}
